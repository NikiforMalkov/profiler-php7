{
  "name": "spiral-packages/xhprof-client",
  "description": "This is my package xhprof-client",
  "keywords": [
    "spiral-packages",
    "spiral",
    "xhprof-client"
  ],
  "homepage": "https://github.com/spiral-packages/xhprof-client",
  "license": "MIT",
  "authors": [
    {
      "name": "butschster",
      "email": "butschster@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "SpiralPackages\\Xhprof\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SpiralPackages\\Xhprof\\Tests\\App\\": "tests/app",
      "SpiralPackages\\Xhprof\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --config=psalm.xml ./src"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
